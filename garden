#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a[] ={1,8,4,9,6};
    int b[] ={1,7,2,1,2};
    int n = 5;
    int i{},prev{},mx{},count{};
    prev = b[i];
    mx = b[i];
    if(a[0]!=b[0])
        count++;
    i=1;
    while(i<n){
        if(a[i]==b[i]){
            // mx = max(b[i],mx);
            // prev = b[i++];
            i++;
            continue;
        }
        if(b[i]==prev){
            i++;
            continue;
        }
        if(b[i]==mx){
            mx = max(b[i],mx);
            prev = b[i++];
            continue;
        }
        mx = max(b[i],mx);
        prev = b[i++];
        count++;
    }
    cout<<count;

    return 0;
}
